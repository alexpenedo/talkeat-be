{
  "testURL": "http://localhost",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "/e2e/.*\\.(e2e.test).(ts|tsx|js)$",
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,tsx,ts}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageReporters": [
    "json",
    "lcov"
  ]
}